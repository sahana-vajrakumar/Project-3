<h1>Your List Of Favourite</h1>
<div class="row">
  <% @movie.each do |movie| %>

    <div class="col-md-3">

      <img src="<%= movie.image%>">
      <div class="caption">
        <br>

        <h1 class="history">Title:
          <%= movie.name %></h1>

        <h1 class="history">Year:
          <%= movie.year %></h1>

      </div>
    </div>

  <% end %>
</div>
